<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:schemaLocation="https://www.phpdoc.org https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd">
    <title>lkrms/util</title>
    <paths>
        <output>build/api</output>
        <cache>build/cache/api</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn=".">
                <path>src</path>
                <!--<path>lib/lk-util</path>-->
            </source>
            <ignore>
                <path>**/*.generated.*</path>
            </ignore>
            <visibility>public</visibility>
            <visibility>protected</visibility>
            <default-package-name>Lkrms</default-package-name>
            <ignore-tags>
                <ignore-tag>lkrms-generate-command</ignore-tag>
                <ignore-tag>lkrms-sample-entity</ignore-tag>
                <ignore-tag>psalm-param</ignore-tag>
                <ignore-tag>psalm-return</ignore-tag>
                <ignore-tag>psalm-template</ignore-tag>
                <ignore-tag>template</ignore-tag>
            </ignore-tags>
        </api>
    </version>
    <setting
            name="graphs.enabled"
            value="true" />
</phpdocumentor>