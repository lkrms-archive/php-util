<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:schemaLocation="https://www.phpdoc.org https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd">
    <title>lkrms/util</title>
    <paths>
        <output>docs/api</output>
        <cache>build/cache/api</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <ignore>
                <path>**/*.generated.*</path>
            </ignore>
            <visibility>public</visibility>
            <visibility>protected</visibility>
            <default-package-name>Lkrms</default-package-name>
            <include-source>true</include-source>
            <ignore-tags>
                <ignore-tag>property</ignore-tag>
                <ignore-tag>property-read</ignore-tag>
                <ignore-tag>property-write</ignore-tag>
                <ignore-tag>lkrms-generate-command</ignore-tag>
                <ignore-tag>lkrms-reference-entity</ignore-tag>
                <ignore-tag>phpstan-impure</ignore-tag>
                <ignore-tag>phpstan-param</ignore-tag>
                <ignore-tag>phpstan-return</ignore-tag>
                <ignore-tag>phpstan-var</ignore-tag>
            </ignore-tags>
        </api>
        <guide>
            <source>
                <path>docs</path>
            </source>
            <output>guide</output>
        </guide>
    </version>
    <setting
            name="guides.enabled"
            value="true" />
</phpdocumentor>