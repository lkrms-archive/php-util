#!/usr/bin/env php
<?php

namespace Lkrms\Tests;

use Lkrms\Console\Console;
use Lkrms\Err;
use Throwable;
use UnexpectedValueException;

$loader = require (__DIR__ . "/../vendor/autoload.php");
$loader->addPsr4("Lkrms\\Tests\\", __DIR__);

Err::handleErrors();

function returnCaller()
{
    return Err::getCaller();
}

Console::debug("Err::getCaller()", returnCaller(), null, 0);

Err::handleErrors(null, '@^' . preg_quote(dirname(__DIR__)) . '/vendor/adodb/adodb-php/@');

throw new UnexpectedValueException("Well, this was unexpected");
